{
  "manifest_version": 2,
  "name": "Huobi market",
  "version":"1.0.2",
  "homepage_url": "https://github.com/jsonwu5/huobi-market",
  "description": "A Huobi market Browser Extension",
  "default_locale": "zh_CN",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "火币行情chrome扩展",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
